# Docker Compose Environment Variables
# Copy this file to .env to customize your deployment

# Application Environment
NODE_ENV=development
LOG_LEVEL=info
ALLOW_DOCKER_SEED=true
SEED_DB_ON_START=false

# External API Keys (Optional - leave empty to use mock data)
# Gate.io API credentials
GATE_API_KEY=
GATE_API_SECRET=

# KyberSwap RPC URL
KYBER_RPC_URL=

# Use mock markets (set to false when you have real API keys)
USE_MOCK_MARKETS=true

# Error Tracking (Optional)
# Sentry DSN for error monitoring
SENTRY_DSN=

# Trading Configuration
MIN_SPREAD_THRESHOLD=1.5
MAX_INVESTMENT=10000
SCAN_INTERVAL=60

# Worker Configuration
WORKER_CONCURRENCY=3
WORKER_JOB_TIMEOUT_MS=60000
OPPORTUNITY_TTL_SECONDS=300

# Frontend Configuration
VITE_API_BASE_URL=http://localhost:4000/api
VITE_APP_NAME=Arbitrage Dashboard
VITE_APP_VERSION=1.0.0

# CORS (comma-separated list of allowed origins)
ALLOWED_ORIGINS=http://localhost:3000
