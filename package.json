{
  "name": "workspace",
  "version": "1.0.0",
  "description": "pnpm workspace with backend, frontend, worker, and shared packages",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": "20.10.0"
  },
  "scripts": {
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "dev": "pnpm -r --parallel dev",
    "clean": "pnpm -r clean"
  },
  "pnpm": {
    "overrides": {},
    "peerDependencyRules": {
      "ignoreMissing": []
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3"
  }
}
